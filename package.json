{
  "name": "@qiscus-integration/react-native-multichannel-widget",
  "version": "0.1.2",
  "description": "RN multichannel widget (Build in UI)",
  "main": "lib/index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "config:dev": "json -I -f package.json -e 'this.main=\"node_modules/expo/AppEntry.js\"'",
    "config:npm": "json -I -f package.json -e 'this.main=\"lib/index.js\"'",
    "commit": "git-cz",
    "release": "standard-version",
    "pack": "npm run config:npm && npm pack ./"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.8.0",
    "axios": "^0.20.0",
    "html-entities": "^1.3.1",
    "localstorage-polyfill": "^1.0.1",
    "lodash.debounce": "^4.0.8",
    "mitt": "^2.1.0",
    "moment": "^2.29.1",
    "qiscus-sdk-core": "^2.10.4",
    "react-native-hyperlink": "^0.0.19",
    "react-native-image-progress": "^1.1.1",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-progress": "^4.1.2",
    "react-native-root-siblings": "^4.0.6",
    "react-native-root-toast": "^3.2.1",
    "xstream": "^11.14.0"
  },
  "devDependencies": {
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "expo-image-picker": "~9.1.0",
    "json": "^10.0.0",
    "standard-version": "^9.0.0",
    "expo": "~39.0.2",
    "expo-status-bar": "~1.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.4.tar.gz",
    "react-native-web": "~0.13.12",
    "@babel/core": "~7.9.0",
    "react-native-dotenv": "^0.2.0"
  },
  "private": false,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Qiscus-Integration/rn-multichannel-widget.git"
  },
  "keywords": [
    "qiscus",
    "multichannel"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Qiscus-Integration/rn-multichannel-widget/issues"
  },
  "homepage": "https://github.com/Qiscus-Integration/rn-multichannel-widget#readme"
}
